<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox id="myFilesBg" prefHeight="776.0" prefWidth="781.0" stylesheets="@../css/backgroundc.css" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="">
   <children>
      <HBox alignment="CENTER">
         <children>
            <ImageView fx:id="directoryLogo" fitHeight="183.0" fitWidth="273.0" pickOnBounds="true" preserveRatio="true" />
         </children>
         <VBox.margin>
            <Insets left="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER" spacing="10.0" style="-fx-background-color: #4B4C57;">
         <children>
            <Label text="Find just the right file" textFill="WHITE">
               <FlowPane.margin>
                  <Insets right="10.0" />
               </FlowPane.margin>
               <font>
                  <Font name="Khmer UI Bold" size="12.0" />
               </font>
            </Label>
            <ChoiceBox prefWidth="150.0">
               <FlowPane.margin>
                  <Insets left="10.0" right="10.0" />
               </FlowPane.margin>
            </ChoiceBox>
            <TextField prefHeight="25.0" prefWidth="164.0" promptText="keyword" />
            <Button mnemonicParsing="false" style="-fx-background-radius: 0;" text="Search">
               <FlowPane.margin>
                  <Insets left="10.0" />
               </FlowPane.margin>
            </Button>
         </children>
         <padding>
            <Insets bottom="5.0" top="5.0" />
         </padding>
      </HBox>
      <Text fill="#616161" strokeType="OUTSIDE" strokeWidth="0.0" text="URL Tool" underline="true">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="16.0" />
         </font>
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" top="20.0" />
         </VBox.margin>
      </Text>
      <HBox alignment="CENTER_LEFT" spacing="5.0">
         <children>
            <Button mnemonicParsing="false" style="-fx-background-radius: 0;" text="Up directory">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Button>
            <Label fx:id="curDirLbl" onMouseClicked="#pathToClipboard" />
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="20.0" />
         </VBox.margin>
      </HBox>
      <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </BorderPane>
      <HBox alignment="CENTER_LEFT" spacing="10.0">
         <children>
            <Label fx:id="filePathLabel" onMouseClicked="#pathToClipboard" />
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="20.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <BorderPane prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #090a0c,        linear-gradient(#38424b 0%, #1f2429 20%, #191d22 100%),        linear-gradient(#20262b, #191d22),        radial-gradient(center 50% 0%, radius 100%, rgba(114,131,148,0.9), rgba(255,255,255,0));">
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin>
      </BorderPane>
   </children>
</VBox>
